create table userdata( 
first_name varchar2(20), 
middle_name varchar2(20), 
city varchar2(40) 
);

insert into userdata values('bala','krishna','uk');

select * from userdata;

select concat(first_name,middle_name)as fullname from userdata;

select INITCAP('my first tutorial') from dual;

select upper('my first tutorial') from dual;

select lower('my first REREIR') from dual;

select LTRIM('                  my first tutorial             ') from dual;

select rtrim('                  my first tutorial             ') from dual;

select rtrim(LTRIM('                  my first tutorial             ')) as name  from dual;

select trim('                       my first tutorrial            ') from dual;

select replace('bala@gmail.com','gamil','yahoo') from dual;

select substr('AERNEAURARU',3,5) from dual;

select substr('ABCDEFGHIJKLMNOPQRSTUVWXYZ',1,5) from dual;

select substr('ABCDEFGHIJKLMNOPQRSTUVWXYZ',-1,5) from dual;

select substr('ABCDEFGHIJ  KLMNOPQRSTUVWXYZ',1,-5) from dual;

select substr('ABCDEFGHIJ  KLMNOPQRSTUVWXYZ',-1,-5) from dual;

select upper(first_name), lower(middle_name) , replace(city,'usa','uk') from userdata;

select upper(first_name)as firstname, lower(middle_name) as middlename , replace(city,'usa','uk') from userdata;

select upper(first_name)as firstname, lower(middle_name) as middlename , replace(city,'usa','uk') as city from userdata;

select * from userdata;

select upper(first_name)as firstname, lower(middle_name) as middlename , replace(city,'usa','uk') as city substr(middle_name,1,3) as subname from userdata;

select upper(first_name)as firstname, lower(middle_name) as middlename , replace(city,'usa','uk') as city substr(middle_name,1,3) as subname from userdata;

select upper(first_name)as firstname, lower(middle_name) as middlename , replace(city,'usa','uk') as city, substr(middle_name,1,3) as subname from userdata;

select * from userdata;

select length('ABCDEFGHIJKL') from dual;

select length('ABCDE    FGHIJKL') from dual;

select instr('ABCD  EFG HIJK LMNO','D',1) from dual;

select instr('ABCD  EFG HDIJK LMDNO','D',1) from dual;

select instr('ABCD  EFG HDIJK LMDNO','D',1,2) from dual;

select instr('ABCD  EFG HDIJK LMDNO','D',1,4) from dual;

select instr('ABCD  EFG HDIJK LMDNO','DE',1,2) from dual;

select instr('ABCD  EFG HDIJK LMDNO','DE',1) from dual;

select instr('ABCD  EFG HDIJK LMDNO','FG',1) from dual;

select regexp_count('aeineheangurgkaogkrukoaijkagaaohru','a') from dual;

select regexp_count('aeineheangurgkaogkrukoaijkagaaohru','a') as regexcount from dual;

